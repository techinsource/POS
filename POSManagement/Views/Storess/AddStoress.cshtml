
@{
    ViewData["Title"] = "AddStoress";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-12">
    <div class="ibox float-e-margins">

        <div class="ibox-content">


            <div class=" col-lg-10" style=" margin-left:100px;">

                <form id="AddStoress">
                    <input type="hidden" id="Id" name="Id">
                    <div class="form-group">
                        <label>Name:</label>
                        <input type="text" id="Name" name="Name" placeholder="Enter Name" class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Code Name:</label>
                        <input type="text" id="CodeName" name="CodeName" placeholder="Code Name" class="form-control">
                    </div>
                    <div class="form-group">

                        <label class="has-float-label">Country</label>
                        <select class="form-control countrypicker" id="Country" name="Country" data-live-search="true" data-default="Pakistan" data-flag="true"></select>

                    </div>

                    <div class="form-group">
                        <label>Mobile:</label>
                        <input type="number" id="Mobile" name="Mobile" placeholder="Enter Mobile" class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Email:</label>
                        <input type="email" id="Email" name="Email" placeholder="Enter Email" class="form-control">
                    </div>

                    <div class="form-group">
                        <label>ZipCode:</label>
                        <input type="text" id="Zipcode" name="Zipcode" placeholder="Enter Zipcode" class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Address:</label>
                        <input type="text" id="Address" name="Address" placeholder="Enter Address" class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Vant Reg no:</label>
                        <input type="text" id="VantRegno" name="VantRegno" placeholder="Enter VantRegno" class="form-control">
                    </div>
                    <div class="form-group">
                        <label class="has-float-label">Select User</label>
                        <select class="form-control " id="Userss" name="Userss">
                            @foreach (var item in ViewBag.Userss)
                            {
                                <option value=@item.Id>@item.Name</option>
                            }
                        </select>
                    </div>

                    <div class="form-group">
                        <label>Time:</label>
                        <input type="time" id="Timezo" name="Timezo" placeholder="Enter Timezo" class="form-control">
                    </div>
                    <div class="form-group col-sm-6">
                        <label>  Invoice Edit Lifespan </label>

                        <input type="number" class="form-control" id="invoice_edit_lifespan" value="4320" name="">
                    </div>
                    <div class="form-group col-sm-6">
                        <label>  Select Minuts Or Secound </label>
                        <select class="form-control select2-hidden-accessible" name="" id="invoice_edit_lifespan_unit" tabindex="-1" aria-hidden="true">
                            <option selected="selected" disabled="" hidden="" value="">
                                --- Please Select ---
                            </option>
                            <option value="minute" selected="">Minute</option>
                            <option value="second">Second</option>
                        </select>
                    </div>
                    <div class="form-group col-sm-6">
                        <label>  Invoice Delete Lifespan </label>

                        <input type="number" class="form-control" id="invoice_edit_lifespan" value="4320" name="">
                    </div>
                    <div class="form-group col-sm-6">
                        <label>  Select Minuts Or Secound </label>
                        <select class="form-control select2-hidden-accessible" name="" id="invoice_edit_lifespan_unit" tabindex="-1" aria-hidden="true">
                            <option selected="selected" disabled="" hidden="" value="">
                                --- Please Select ---
                            </option>
                            <option value="minute" selected="">Minute</option>
                            <option value="second">Second</option>
                        </select>
                    </div>

                    <div class="form-group">

                        <label class="has-float-label">After Sell Page:</label>

                        <select class="form-control" id="Aftersellp" name="Aftersellp">

                            <option value="POS">POS</option>
                            <option value="Invoice">Invoice</option>
                        </select>
                    </div>

                    <div class="form-group">
                        <label>Tax <span data-toggle="tooltip" title="" data-original-title="Tax"></span></label>

                        <input type="number" id="Text" name="Text" value="0" class="form-control">
                    </div>

                    <div class="form-group">
                        <label>Stock Alert Quantity:</label>
                        <input type="number" id="Stockalertq" name="Stockalertq" class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Datatable Item Limit:</label>
                        <input type="number" id="Itemlimit" name="Itemlimit" value="25" class="form-control">
                    </div>
                    <div class="form-group">
                        <label>Invoice Footer Text:</label>
                        <textarea rows="3" cols="50" id="Footer" name="Footer" class="form-control"></textarea>
                    </div>
                    <div class="form-group">

                        <label class="has-float-label">Sound Effect</label>

                        <select class="form-control" id="Soundeffect" name="Soundeffect">

                            <option value="Active">Active</option>
                            <option value="Inactive">Inactive</option>
                        </select>
                    </div>


                    <div>
                        <button class="btn btn-sm btn-primary pull-right m-t-n-xs" type="button" id="InsertStoress" onclick="AddStoress()" value="Save"><strong>Save</strong></button>


                    </div>
                </form>


            </div>
            <table class="table">
                <tr>
                    <th>Name</th>
                    <th>Country</th>
                    <th>Address</th>
                    <th>Created At</th>

                    <th>Action</th>
                </tr>
                <tbody id="GetStoress"></tbody>
            </table>
        </div>
    </div>


    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
    <script src="~/js/countrypicker.js"></script>
